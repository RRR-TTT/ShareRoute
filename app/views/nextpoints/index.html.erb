<p id="notice"><%= notice %></p>

<h1>Listing Nextpoints</h1>

<table>
  <thead>
    <tr>
      <th>Nextpointname</th>
      <th>Nextpointrequiredtime</th>
      <th>Nextpointtransportation</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @nextpoints.each do |nextpoint| %>
      <tr>
        <td><%= nextpoint.NextpointName %></td>
        <td><%= nextpoint.NextpointRequiredTime %></td>
        <td><%= nextpoint.NextpointTransportation %></td>
        <td><%= link_to 'Show', nextpoint %></td>
        <td><%= link_to 'Edit', edit_nextpoint_path(nextpoint) %></td>
        <td><%= link_to 'Destroy', nextpoint, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Nextpoint', new_nextpoint_path %>
