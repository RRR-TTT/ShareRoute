<p id="notice"><%= notice %></p>

<h1>Listing User Routes</h1>

<table>
  <thead>
    <tr>
      <th>Total time</th>
      <th>Userroutename</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_routes.each do |user_route| %>
      <tr>
        <td><%= user_route.total_time %></td>
        <td><%= user_route.UserRouteName %></td>
        <td><%= link_to 'Show', user_route %></td>
        <td><%= link_to 'Edit', edit_user_route_path(user_route) %></td>
        <td><%= link_to 'Destroy', user_route, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User route', new_user_route_path %>
