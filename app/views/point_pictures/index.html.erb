<p id="notice"><%= notice %></p>

<h1>Listing Point Pictures</h1>

<table>
  <thead>
    <tr>
      <th>Pointpictureimage</th>
      <th>Postinguser</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @point_pictures.each do |point_picture| %>
      <tr>
        <td><%= point_picture.PointPictureImage %></td>
        <td><%= point_picture.PostingUser %></td>
        <td><%= link_to 'Show', point_picture %></td>
        <td><%= link_to 'Edit', edit_point_picture_path(point_picture) %></td>
        <td><%= link_to 'Destroy', point_picture, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Point picture', new_point_picture_path %>
